{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",

  "name": "${mod_name}",
  "description": "${description}",
  "authors": [
    "${mod_author}"
  ],
  "contact": {
    "sources": "https://github.com/hablethedev/moosescript",
    "issues": "https://github.com/hablethedev/moosescript/issues"
  },

  "license": "${license}",
  "icon": "assets/modid/moosescript-logo.png",

  "environment": "client",
  "entrypoints": {
    "main": [
      "dev.moosescript.fabric.MoosescriptFabricMod"
    ],
    "client": [
      "dev.moosescript.fabric.MoosescriptFabricClientMod"
    ]
  },

  "mixins": [
    "moosescript.mixins.json",
    "moosescript.fabric.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "fabric": "*",
    "fabric-screen-api-v1": "*",
    "minecraft": "${minecraft_version}",
    "java": ">=${java_version}"
  },
  "suggests": {
    "another-mod": "*"
  }
}
